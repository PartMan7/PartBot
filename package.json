{
	"name": "partbot",
	"version": "3.0.0",
	"description": "A chatbot for Pok√©mon Showdown",
	"main": "src/index.js",
	"scripts": {
		"build-configs": "bash ./scripts/build_nginx_confs.sh",
		"lint": "eslint src --ext .ts",
		"lint:fix": "eslint src --ext .ts --fix",
		"notify-unpushed": "sh scripts/notify_unpushed.sh",
		"secrets-check": "sh scripts/secrets_check.sh",
		"prettify": "prettier -w",
		"prepare": "sh ./scripts/setup.sh",
		"prestart": "bun run webpack && bun run tailwind",
		"start": "BUN_JSC_useTemporal=1 bun src/index.ts",
		"tailwind": "npx @tailwindcss/cli -i ./src/web/styles/tailwind.css -o ./src/web/react/compiled/styles.css",
		"test": "bun run lint && bunx tsc",
		"webpack": "webpack -c src/web/webpack/webpack.config.ts"
	},
	"engines": {
		"bun": "^1.2.17"
	},
	"keywords": [],
	"author": "PartMan7",
	"license": "ISC",
	"dependencies": {
		"@tailwindcss/cli": "^4.0.8",
		"chess.js": "^1.2.0",
		"chokidar": "^3.5.3",
		"cron": "^4.3.0",
		"discord.js": "^14.11.0",
		"express": "^5.0.1",
		"flat-cache": "^6.1.1",
		"mongoose": "^8.12.1",
		"ps-client": "^4.5.3",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"sheets-parser": "^1.1.1",
		"tailwindcss": "^4.0.8",
		"webpack": "^5.98.0",
		"webpack-cli": "^6.0.1"
	},
	"devDependencies": {
		"@types/bun": "latest",
		"@types/express": "^5.0.0",
		"@types/node": "^20.4.4",
		"@types/react": "^18.2.48",
		"@types/react-dom": "^18.2.18",
		"@types/webpack": "^5.28.5",
		"@typescript-eslint/eslint-plugin": "^6.2.0",
		"@typescript-eslint/parser": "^6.2.0",
		"eslint": "^8.45.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-import-resolver-typescript": "^3.6.3",
		"eslint-plugin-import": "^2.31.0",
		"husky": "^8.0.3",
		"prettier": "^3.3.3",
		"temporal-spec": "^0.3.0"
	},
	"peerDependencies": {
		"typescript": "^5"
	}
}
